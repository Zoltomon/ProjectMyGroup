<Page x:Class="ShippingCompany.Page.Profile.UserProfile"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:ShippingCompany.Page.Profile"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="UserProfile">

    <Grid>
        <StackPanel>
            <TextBlock  Name="TxbUser" Text="Имя пользователя" Style="{StaticResource TxbName}"/>
            <Image/>
            <Button Content="Назад" Name="btnBack" Click="btnBack_Click"  Style="{StaticResource BtnAll}"/>
        </StackPanel>
    </Grid>
</Page>
