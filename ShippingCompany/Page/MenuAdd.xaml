<Page x:Class="ShippingCompany.Page.MenuAdd"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:ShippingCompany.Page"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="MenuAdd">

    <Grid>
        <StackPanel VerticalAlignment="Center">
            <TextBlock Name="TxbLoginUser" Text="Имя пользователя" Style="{StaticResource TxbName}"/>
            <Button Content="Добавление пользователя" Name="BtnUser"/>
            <Button Content="Добавление о круизе" Name="BtnCruise"/>
            <Button Content="Добавление о порте" Name="Btn"/>
            <Button Content="Назад" Name="Btnback" Click="Btnback_Click"/>
        </StackPanel>
    </Grid>
</Page>
